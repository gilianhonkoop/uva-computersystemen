<?xml version="1.0" encoding="ISO-8859-1"?>
<SIMPLE NAME="Control" PREFERREDNUMBERFORMAT="Hexadecimal" PREFERREDCYCLELENGTH="10">
    <PARAMETERS/>
    <FORMS>
        <LINE COLOR="#000000" FROMX="260" FROMY="180" TOX="260" TOY="350">
            <STROKE LINEWIDTH="3.0"/>
        </LINE>
    </FORMS>
    <IO>
        <INPUT BITS="15" SIGNED="false" POSX="260.0" POSY="270.0" NAME="Instruction">
            <LABEL POSX="-10.0" POSY="0.0" HALIGN="RIGHT" VALIGN="CENTER"/>
        </INPUT>
        <OUTPUT BITS="4" SIGNED="false" POSX="260.0" POSY="250.0" NAME="RS">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="4" SIGNED="false" POSX="260.0" POSY="310.0" NAME="RT">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="4" SIGNED="false" POSX="260.0" POSY="350.0" NAME="RD">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="3" SIGNED="false" POSX="260.0" POSY="180.0" NAME="S">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
    </IO>
    <MEMORY/>
    <INTERNALS DELAY="1">
        <ACTION EVENT="INPUT_CHANGE">
{
	S =    Instruction &gt;&gt; 0d12;
	RS = 	Instruction &gt;&gt; 0d8;
	RT = 	Instruction &gt;&gt; 0d4;
	RD = 	Instruction ;

}
		</ACTION>
    </INTERNALS>
</SIMPLE>
