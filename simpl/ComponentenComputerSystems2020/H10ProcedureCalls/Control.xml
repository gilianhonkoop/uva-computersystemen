<?xml version="1.0" encoding="ISO-8859-1"?>
<SIMPLE NAME="Control" PREFERREDNUMBERFORMAT="Hexadecimal" PREFERREDCYCLELENGTH="10">
    <PARAMETERS/>
    <FORMS>
        <LINE COLOR="#000000" FROMX="260" FROMY="30" TOX="260" TOY="295">
            <STROKE LINEWIDTH="2.0" ENDCAP="0"/>
        </LINE>
    </FORMS>
    <IO>
        <INPUT BITS="39" SIGNED="false" POSX="261.0" POSY="270.0" NAME="Instruction">
            <LABEL POSX="-10.0" POSY="0.0" HALIGN="RIGHT" VALIGN="CENTER"/>
        </INPUT>
        <OUTPUT BITS="1" SIGNED="false" POSX="260.0" POSY="40.0" NAME="MemWrite">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="2" SIGNED="false" POSX="261.0" POSY="50.0" NAME="ResultOrigin">
            <LABEL POSX="-80.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="1" SIGNED="false" POSX="261.0" POSY="60.0" NAME="Branch">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="1" SIGNED="false" POSX="260.0" POSY="70.0" NAME="Invert">
            <LABEL POSX="-40.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="3" SIGNED="false" POSX="261.0" POSY="100.0" NAME="ALUOp">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="1" SIGNED="false" POSX="261.0" POSY="90.0" NAME="SecReg">
            <LABEL POSX="-50.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="1" SIGNED="false" POSX="261.0" POSY="80.0" NAME="RegWrite">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="16" SIGNED="false" POSX="261.0" POSY="170.0" NAME="IMM">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="4" SIGNED="false" POSX="261.0" POSY="235.0" NAME="RS">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="4" SIGNED="false" POSX="261.0" POSY="255.0" NAME="RT">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="4" SIGNED="false" POSX="261.0" POSY="295.0" NAME="RD">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="1" SIGNED="false" POSX="260.0" POSY="30.0" NAME="FirstReg">
            <LABEL POSX="-50.0" POSY="-10.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
    </IO>
    <MEMORY/>
    <INTERNALS DELAY="1">
        <ACTION EVENT="INPUT_CHANGE">{
	0d8 bit Op = Instruction &gt;&gt; 0d31;
	FirstReg =  Op &gt;&gt; 0d7;
	MemWrite = 	Op &gt;&gt; 0d6;
	ResultOrigin = 	Op &gt;&gt; 0d4;
	Branch = 	Op &gt;&gt; 0d3;
	Invert =	Op &gt;&gt; 0d2;
	RegWrite = 	Op &gt;&gt; 0d1;
	SecReg =  Op &gt;&gt; 0d0;

	ALUOp = 	Instruction &gt;&gt; 0d28;

	RS = 	Instruction &gt;&gt; 0d24;
	RT = 	Instruction &gt;&gt; 0d20;
	RD = 	Instruction &gt;&gt; 0d16;

	IMM = 	Instruction;
}
		</ACTION>
    </INTERNALS>
</SIMPLE>
